{
  "include": [
    "src/**/*"
  ],
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "baseUrl": "./src",
    "paths": {
        "@constants/*": [
          "app/constants/*",
        ],
        "@models/*": [
          "app/models/*",
        ]
    }
  }
}
